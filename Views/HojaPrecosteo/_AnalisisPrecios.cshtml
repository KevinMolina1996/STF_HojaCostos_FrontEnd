@model AppWebHojaCosto.Models.HOJA_COSTO_PRECOSTEO

<div>
    <fieldset style="border-width:2px; border-style:groove; padding:10px;">
        <legend style="padding: 2px; width:auto;">Analisis de Precios por Destino de Distribucion</legend>
        <div class="row">
            <div class="col-md-4 offset-md-4">
                <span style="font-weight:bold; font-size:18px;">Estimado</span>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-4">
                        @Html.HiddenFor(x => x.TBL_PRE_ANALISIS_PREC.PreApCodigoN)
                        @Html.LabelFor(m => m.TBL_PRE_ANALISIS_PREC.PreApPrecMaxIvaEstD, new { @class = "control-label" })
                    </div>
                    <div class="col-md-4">
                        <div class="input-group mb-1">
                            @Html.TextBoxFor(m => m.TBL_PRE_ANALISIS_PREC.PreApPrecMaxIvaEstD, new { name = "PreApPrecMaxIvaEstD", id = "PreApPrecMaxIvaEstD", @class = "form-control input-sm solonumeros" })
                            <div class="input-group-append">
                                <span class="input-group-text">%</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        @Html.LabelFor(m => m.TBL_PRE_ANALISIS_PREC.PreApMargenMaxBrutoEstF, new { @class = "control-label" })
                    </div>
                    <div class="col-md-4">
                        <div class="input-group mb-1">
                            @Html.TextBoxFor(m => m.TBL_PRE_ANALISIS_PREC.PreApMargenMaxBrutoEstF, new { name = "PreApMargenMaxBrutoEstF", id = "PreApMargenMaxBrutoEstF", @class = "form-control input-sm solonumeros" })
                            <div class="input-group-append">
                                <span class="input-group-text">%</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        @Html.LabelFor(m => m.TBL_PRE_ANALISIS_PREC.PreApMargenMaxOpeEstF, new { @class = "control-label" })
                    </div>
                    <div class="col-md-4">
                        <div class="input-group mb-1">
                            @Html.TextBoxFor(m => m.TBL_PRE_ANALISIS_PREC.PreApMargenMaxOpeEstF, new { name = "PreApMargenMaxOpeEstF", id = "PreApMargenMaxOpeEstF", @class = "form-control input-sm solonumeros" })
                            <div class="input-group-append">
                                <span class="input-group-text">%</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <hr />
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-4">
                        @Html.LabelFor(m => m.TBL_PRE_ANALISIS_PREC.PreApPrecMinIvaEstD, new { @class = "control-label" })
                    </div>
                    <div class="col-md-4">
                        <div class="input-group mb-1">
                            @Html.TextBoxFor(m => m.TBL_PRE_ANALISIS_PREC.PreApPrecMinIvaEstD, new { name = "PreApPrecMinIvaEstD", id = "PreApPrecMinIvaEstD", @class = "form-control input-sm solonumeros" })
                            <div class="input-group-append">
                                <span class="input-group-text">%</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        @Html.LabelFor(m => m.TBL_PRE_ANALISIS_PREC.PreApMargenMinBrutoEstF, new { @class = "control-label" })
                    </div>
                    <div class="col-md-4">
                        <div class="input-group mb-1">
                            @Html.TextBoxFor(m => m.TBL_PRE_ANALISIS_PREC.PreApMargenMinBrutoEstF, new { name = "PreApMargenMinBrutoEstF", id = "PreApMargenMinBrutoEstF", @class = "form-control input-sm solonumeros" })
                            <div class="input-group-append">
                                <span class="input-group-text">%</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        @Html.LabelFor(m => m.TBL_PRE_ANALISIS_PREC.PreApMargenMinOpeEstF, new { @class = "control-label" })
                    </div>
                    <div class="col-md-4">
                        <div class="input-group mb-1">
                            @Html.TextBoxFor(m => m.TBL_PRE_ANALISIS_PREC.PreApMargenMinOpeEstF, new { name = "PreApMargenMinOpeEstF", id = "PreApMargenMinOpeEstF", @class = "form-control input-sm solonumeros" })
                            <div class="input-group-append">
                                <span class="input-group-text">%</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </fieldset>
