@model AppWebHojaCosto.Models.DASHBOARDLVL2

<div class="card mb-3">
    <div class="card-header">
        <i class="fa fa-table"></i> No Producido
    </div>
    <div class="card-body" style="text-align:center;">
        <div class="table-responsive">
            <table class="table table-sm table-bordered" id="tablaNoProducido" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>REFERENCIA</th>
                        <th>DESCRIPCIÓN</th>
                        <th>LÍNEA</th>
                        <th>FECHA OC</th>
                        <th>ORDEN DE COMPRA</th>
                        <th>ESTADO OC</th>
                        <th>COSTO COMPRA</th>
                        <th>MONEDA</th>
                        <th>TRM</th>
                        <th>COSTO COMPRA COP</th>
                        <th>CANTIDAD</th>
                        <th>PROVEEDOR</th>
                        <th>TÉRMINO DE NEGOCIACIÓN</th>
                        <th>PAÍS ORIGEN</th>
                        <th>ESTADO</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model._DASH_NO_PRODUCIDO != null)
                    {
                        foreach (var item in Model._DASH_NO_PRODUCIDO)
                        {
                            <tr>
                                <td>@item.Referencia</td>
                                <td>@item.Descripcion</td>
                                <td>@item.Linea</td>
                                <td>@item.FechaOc</td>
                                <td>@item.OrdenCompra</td>
                                <td>@item.EstadoOc</td>
                                <td>@item.CostoCompra</td>
                                <td>@item.Moneda</td>
                                <td>@item.Trm</td>
                                <td>@item.CostoCompraCop</td>
                                <td>@item.Cantidad</td>
                                <td>@item.Proveedor</td>
                                <td>@item.TerminoNegociacion</td>
                                <td>@item.PaisOrigen</td>
                                <td>@item.Estado</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
            @*<div style="text-align:center; color:#ffffff;">
                <a class="btn btn-sm btn-dark" id="exportNoProducido">
                    <span class="fa fa-download"></span> Descargar
                </a>
            </div>*@
        </div>
    </div>
</div>