    <style>
        /* clearfix */
        tspan {
            font-size: 10px !important;
        }

        .clear:before,
        .clear:after {
            content: "";
            display: table;
        }

        .clear:after {
            clear: both;
        }

        .clear {
            *zoom: 1;
        }

        .wrapper {
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            margin: 30px;
            border: 1px solid #cccccc;
        }

        .gauge {
            display: block;
            float: left;
        }

        #g1 {
            width: 100%;
        }

        #g2 {
            width: 100%;
        }
    </style>
    <script>

    </script>
}

